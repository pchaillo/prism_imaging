function all_peaks = compute_all_spectra(bio_dat)

l = length(bio_dat);

spectra_tab_raw = [];

for i = 1 : 100 %length(bio_dat)
    if bio_dat(i).num > 0
        spectra_tab_raw = [ spectra_tab_raw ; bio_dat(i).peaks.mz ];
    end
    X = sprintf('Spectra creation : %d / %d',i,length(bio_dat));
    disp(X)
end

